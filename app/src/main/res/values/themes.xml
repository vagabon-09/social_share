<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.ShareBySocial" parent="Theme.Material3.DayNight.NoActionBar">

    </style>

    <style name="Theme.ShareBySocial" parent="Base.Theme.ShareBySocial" />

    <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="Theme.AppCompat.DayNight.NoActionBar">
        <item name="cornerSize">50%</item>
    </style>

    <style name="daySwitch">
        <item name="android:trackTint">@color/blue</item>
    </style>

    <style name="backGround">
        <item name="android:background">@color/blue</item>
    </style>

    <style name="BottomNav">
        <item name="android:background">@color/white</item>
        <item name="itemIconTint">@color/night_bg</item>
    </style>

    <style name="textColor">
        <item name="color">@color/air_blue</item>
    </style>

    <style name="iconTint">
        <item name="android:tint">#002244</item>
    </style>

    <style name="infoSheetBg">
        <item name="android:background">@color/white</item>
    </style>

    <style name="btnTextColor">
        <item name="android:textColor">@color/blue</item>
    </style>

    <style name="btnBackGround">
        <item name="android:background">@drawable/save_btn</item>
        <item name="backgroundTint">@color/blue</item>
    </style>

    <style name="cancelBtnBg">
        <item name="android:background">@drawable/cancle_btn</item>
        <item name="android:textColor">@color/blue</item>
    </style>

</resources>